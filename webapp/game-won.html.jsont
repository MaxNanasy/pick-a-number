<!doctype html>
<html>
  <head>
    <!-- TODO: Factor out redundant shit -->
    <title>Pick a Number</title>
  </head>
  <body>
    <h1>You've won!</h1>
    <p>Total guesses: {guessesCount|str}</p>
    {.section guesses}
      <h1>Guesses</h1>
      <ul>
        {.repeated section @}
          <li>{@|str}</li>
        {.end}
      </ul>
    {.end}
  </body>
</html>
